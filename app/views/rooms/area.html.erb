<% @results.each do |room| %>
  <tr>
    <td class="align-middle">
      <%= attachment_image_tag room, :image, size:"200x200"%>
    </td>
    <td class="align-middle">
      <%= "#{room.user.username}" %>
    </td>
    <td class="align-middle">
      <%= link_to "#{room.title}", "/rooms/#{room.id}", method: :get %>
    </td>
    <td class="align-middle">
      <%= room.body %></td>
    <td class="align-middle"><%= "¥#{room.cost}円 / 人"%></td>
      <td class="align-middle"><%= "#{room.place}" %>
    </td>
  </tr>
<% end %>